{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "framework": "vue",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/eqsage.js": {
      "memory": 1000,
      "maxDuration": 5
    },
    "api/remoteapi.js": {
      "memory": 1000,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/eq-asset-preview-master/(.*)",
      "destination": "https://raw.githubusercontent.com/Akkadius/eq-asset-preview/master/$1"
    },
    { "source": "/eqsage", "destination": "/api/eqsage" },
    {
      "source": "/remoteapi/(.*)",
      "destination": "/api/remoteapi"
    },
    { "source": "/static/(.*)", "destination": "https://eqsage.vercel.app/static/$1" }
  ]
}